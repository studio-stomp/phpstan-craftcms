{
    "name": "studio-stomp/phpstan-craftcms",
    "type": "phpstan-extension",
    "description": "Craft CMS extension for PHPStan",
    "keywords": [
        "studio-stomp",
        "phpstan-craftcms"
    ],
    "homepage": "https://github.com/studio-stomp/phpstan-craftcms",
    "license": "MIT",
    "authors": [
        {
            "name": "Studio Stomp",
            "email": "hello@studiostomp.nl",
            "homepage": "https://studiostomp.nl/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4|^8.0",
        "phpstan/phpstan": "^1.3.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "scripts": {
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
